<% flash.each do |type, message| %>
    <div class="alert alert-<%= type == 'alert' ? 'danger' : 'primary' %> fade-in">
        <%= message %>


        <button type="button" class="close" data-dismiss=" aria-label="close">
            <span aria-hidden="true">&times;</span>
        </button>
    </div>
<% end %>